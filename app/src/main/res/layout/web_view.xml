<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff">


    <include
        android:id="@+id/app_toobar"
        layout="@layout/tool_barlayout" />


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/rlSupportTittle"
        android:layout_below="@+id/app_toobar">

        <ProgressBar
            android:id="@+id/progressBar_splsh"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_centerHorizontal="true" />

        <TextView
            android:id="@+id/tvSupportTittle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/app_margin"
            android:text="@string/details"
            android:visibility="gone"
            android:textSize="18sp"
            android:textColor="@color/black_john" />
    </RelativeLayout>

    <WebView
        android:id="@+id/webView1"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_below="@+id/rlSupportTittle"

        />
</RelativeLayout>